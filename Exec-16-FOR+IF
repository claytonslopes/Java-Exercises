import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int x, i, in, out, N;
		
		x = sc.nextInt();
		in = 0;
		out = 0;

		for (i=0; i<x; i++) {
			N = sc.nextInt();
				if (N >= 10 && N <= 20) {
					in += 1; 
				}
				else {
					out += 1;
				}
		}
			System.out.println(in + " in");
			System.out.println(out + " out");

		sc.close();
	}

}
