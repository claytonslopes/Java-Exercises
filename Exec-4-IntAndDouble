import java.util.Scanner;

public class Main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int ID, H;
		Double hValue, salary;
		
		ID = sc.nextInt();
		H = sc.nextInt();
		hValue = sc.nextDouble();
		
		salary = H * hValue;

		System.out.println("NUMBER = " + ID);
		System.out.printf("SALARY = U$ %.2f%n", salary);
		
		sc.close();
	}

}
